{
	"name": "@redactie/wem-module",
	"version": "1.0.2",
	"description": "redactie wem module",
	"main": "dist/redactie-wem-module.umd.js",
	"module": "dist/redactie-wem-module.umd.js",
	"types": "dist/index.d.ts",
	"jsnext:main": "dist/redactie-wem-module.umd.js",
	"scripts": {
		"prepublish": "npm run test && npm run build && npm run docs",
		"build:types": "tsc --emitDeclarationOnly",
		"build": "npm run trans:extract && npm run build:types && webpack --env.prod",
		"build:w": "npm run trans:extract && npm run build:types && webpack -w --env.prod",
		"build:analyse": "npm run build:types && webpack --env.analyse",
		"start": "concurrently --kill-others \"npm run build:w\" \"cd demo && npm run start\"",
		"lint:scss": "stylelint public/**/*.scss --allow-empty-input",
		"lint:ts": "eslint --cache --cache-location .eslintcache --ext .ts,.tsx public",
		"lint": "npm run lint:scss && npm run lint:ts",
		"test": "jest ./public",
		"trans:extract": "./node_modules/.bin/i18next-scanner --config i18next-scanner.config.js",
		"docs": "npx typedoc --entryPoints ./public/index.tsx --entryPoints ./public/lib/api/index.ts --excludePrivate"
	},
	"repository": {
		"type": "git",
		"url": "https://bitbucket.antwerpen.be/scm/wem/redactie-wem_module_react.git"
	},
	"keywords": [
		"Redactie",
		"React",
		"Wem",
		"Module"
	],
	"publishConfig": {
		"@redactie:registry": "https://nexusrepo.antwerpen.be/repository/npm-private"
	},
	"devDependencies": {
		"@a-ui/core": "^4.3.1",
		"@acpaas-ui/react-components": "^3.4.1",
		"@acpaas-ui/react-editorial-components": "^1.5.0",
		"@babel/core": "7.15.5",
		"@babel/plugin-proposal-class-static-block": "7.15.4",
		"@babel/preset-env": "7.15.6",
		"@babel/preset-react": "7.14.5",
		"@babel/preset-typescript": "7.15.0",
		"@commitlint/cli": "8.3.5",
		"@commitlint/config-conventional": "8.3.4",
		"@datorama/akita": "^7.1.1",
		"@redactie/form-renderer-module": "0.17.1",
		"@redactie/module-webpack-plugin": "2.0.0",
		"@redactie/redactie-core": "^2.0.0",
		"@redactie/translations-module": "^1.1.0",
		"@redactie/utils": "^1.2.1",
		"@testing-library/jest-dom": "5.0.2",
		"@testing-library/react": "9.4.0",
		"@types/jest": "24.9.1",
		"@types/lodash": "^4.14.185",
		"@types/react": "16.9.19",
		"@types/react-json-editor-ajrm": "^2.5.3",
		"@types/react-router-dom": "^5.3.3",
		"@types/webpack-env": "^1.15.1",
		"@typescript-eslint/eslint-plugin": "5.9.1",
		"@typescript-eslint/parser": "5.9.1",
		"babel-jest": "25.1.0",
		"clean-webpack-plugin": "3.0.0",
		"concurrently": "5.1.0",
		"eslint": "8.6.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-import": "2.25.4",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.28.0",
		"eslint-plugin-react-hooks": "4.3.0",
		"husky": "3.1.0",
		"i18next-scanner": "^3.0.0",
		"i18next-scanner-typescript": "^1.0.6",
		"jest": "25.1.0",
		"lint-staged": "10.0.8",
		"lodash": "^4.17.21",
		"node-sass": "4.13.1",
		"npm": "6.13.7",
		"prettier": "1.19.1",
		"react-sortable-hoc": "^2.0.0",
		"stylelint": "13.0.0",
		"stylelint-config-standard": "19.0.0",
		"ts-loader": "6.2.1",
		"typedoc": "^0.22.4",
		"typescript": "4.4.3",
		"webpack": "4.41.5",
		"webpack-bundle-analyzer": "3.6.0",
		"webpack-cli": "3.3.10"
	},
	"author": "Yapla",
	"license": "ISC",
	"dependencies": {
		"@redactie/roles-rights-module": "^1.2.2",
		"jsonschema": "^1.4.1",
		"react-json-editor-ajrm": "^2.5.13"
	}
}
